{
  "name": "node-ts-seed",
  "version": "1.0.0",
  "description": "Typescript backend seed.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon --watch './src/**/*.ts' --exec 'ts-node' ./src/index.ts",
    "build": "rm -rf lib && tsc",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha --exit --opts test/unit/mocha.opts -r ts-node/register 'test/unit/**/*.test.ts'",
    "test:integration": "mocha --exit --opts test/integration/mocha.opts -r ts-node/register 'test/integration/**/*.test.ts'",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'"
  },
  "author": "4All Tecnologia EPP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/4alltecnologia/node-ts-seed"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "dotenv": "^8.0.0",
    "elastic-apm-node": "^2.12.1",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "joi": "^14.3.1",
    "knex": "^0.18.3",
    "moment-timezone": "^0.5.25",
    "mysql2": "^1.6.5",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/elastic-apm-node": "^2.7.0",
    "@types/express": "^4.17.0",
    "@types/helmet": "0.0.43",
    "@types/joi": "^14.3.3",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^5.2.7",
    "@types/moment-timezone": "^0.5.12",
    "@types/nock": "^10.0.3",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.2"
  }
}
